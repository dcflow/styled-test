{
  "presets": [
    [
      "env",
      {
        "targets": { "node": 9 },
        "useBuiltIns": true
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-es2015-classes",
    "add-module-exports",
    "dynamic-import-webpack",
    "react-hot-loader/babel"
  ]
}
